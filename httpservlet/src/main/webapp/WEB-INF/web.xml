<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.jsp.httpservlet.RegisterByGet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>RegisterPost</servlet-name>
		<servlet-class>com.jsp.httpservlet.RegisterByPost</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ItemInfo</servlet-name>
		<servlet-class>edu.jsp.httpservlet.get.Item</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>account</servlet-name>
		<servlet-class>edu.jsp.httpservlet.post.Account</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>account</servlet-name>
		<url-pattern>/account</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ItemInfo</servlet-name>
		<url-pattern>/item</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/user register</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RegisterPost</servlet-name>
		<url-pattern>/user-register-post</url-pattern>
	</servlet-mapping>

</web-app>
